@model Finalaplication.Models.Volcontract
@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer Localizer
@{
    ViewData["Title"] = "Delete";
    string idofvol = Model.OwnerID;
}
<h3>
    @Localizer["Volcontract"]
</h3>

<div>

    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Localizer["NumberOfRegistration"]:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NumberOfRegistration)
        </dd>
        <dt class="col-sm-2">
            @Localizer["RegistrationDate"]:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RegistrationDate)
        </dd>
        <dt class="col-sm-2">
            @Localizer["ExpirationDate"]:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ExpirationDate)
        </dd>
    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

    <div class="form-actions no-color">
        <input type="hidden" name="idofvol" value="@idofvol" />
        <input type="submit" value=@Localizer["Delete"] class="btn btn-primary" /> |
        <a href=@Url.Action( "Index", new { idofvol = idofvol } ) class="btn btn-primary">@Localizer["BackToList"]</a>
    </div>
    }
</div>
