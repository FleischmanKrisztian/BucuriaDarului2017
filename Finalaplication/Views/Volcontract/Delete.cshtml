@model Finalaplication.Models.Volcontract
@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer Localizer
@{
    ViewData["Title"] = "Delete";
    string idofvol = Model.OwnerID;
}
<h3>
    @Localizer["Volcontract"]
</h3>
<h4>@Localizer["Message"]</h4>
<div>

    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Localizer["NumberOfRegistration"]:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NumberOfRegistration)
        </dd>
        <dt class="col-sm-2">
            @Localizer["RegistrationDate"]:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RegistrationDate)
        </dd>
        <dt class="col-sm-2">
            @Localizer["ExpirationDate"]:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ExpirationDate)
        </dd>
    </dl>




    <form asp-action="Delete">
        <input type="hidden" name="idofvol" value="@idofvol" />
        <a href=@Url.Action( "Delete", new { idofvol = Model.OwnerID})>@Localizer["Delete"]</a> |
        <a href=@Url.Action( "Index", new { idofvol = Model.OwnerID })>@Localizer["BackToList"]</a>
    </form>

</div>
