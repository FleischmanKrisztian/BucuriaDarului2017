@model Finalaplication.Models.Volcontract

@{
    ViewData["Title"] = "Print";
    string volname = ViewBag.volunteername;
    var volbd = ViewBag.volunteerBd;
    string volCNP = ViewBag.volunteerCNP;
    string voladdress = ViewBag.volunteerAddress;
}

<h1>Print</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayName("Full name")
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayName(volname)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayName("CNP")
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayName(volCNP)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayName("Address")
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayName(voladdress)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NumberOfRegistration)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NumberOfRegistration)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.RegistrationDate)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RegistrationDate)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ExpirationDate)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ExpirationDate)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Back to list", "Index", new { idofvol = Model.OwnerID })
</div>
